
public class HelloWorld {
  public static void main(String[] args) {
    // Een comment
    if(false == true) {
      System.out.println("snor");
    }
    
    System.out.println("Hello World");
  }
  
  public static void main2(String[] args) {
    // Een comment
    System.out.println("Hello World");
  }
}
